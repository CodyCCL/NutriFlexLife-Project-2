<h1>Food</h1>

{{#each foodData}} <!-- Use "foodData" which is the variable passed from your route -->
  <h2>{{Name}}</h2> <!-- Access properties directly without "Food." prefix -->
 
  <ul>
    <li>Name: {{Name}}</li>
    <li>Protien: {{Protien}}g</li>
    <li>Carbs: {{Carbs}}g</li>
    <!-- Include other properties as needed -->
  </ul>
{{/each}}

